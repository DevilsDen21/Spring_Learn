<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns= "http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean id="subjectClass" class="com.udamy.spring.SubjectClass" >
            <constructor-arg value="2" name="startTime"></constructor-arg>
            <constructor-arg value="4" name="endTime"></constructor-arg>
            <constructor-arg value="Math" name="subject"></constructor-arg>
        </bean>

</beans>
